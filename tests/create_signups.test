CREATE TABLE wp_signups ( "signup_id" SERIAL NOT NULL, "domain" VARCHAR(200) NOT NULL DEFAULT '', "path" VARCHAR(100) NOT NULL DEFAULT '', "title" TEXT NOT NULL, "user_login" VARCHAR(60) NOT NULL DEFAULT '', "user_email" VARCHAR(100) NOT NULL DEFAULT '', "registered" TIMESTAMP NOT NULL DEFAULT '0001-01-01 00:00:00', "activated" TIMESTAMP NOT NULL DEFAULT '0001-01-01 00:00:00', "active" TINYINT NOT NULL DEFAULT '0', "activation_key" VARCHAR(50) NOT NULL DEFAULT '', "meta" TEXT , PRIMARY KEY ("signup_id") ); CREATE INDEX wp_signups_activation_key ON wp_signups ( "activation_key" ); CREATE INDEX wp_signups_user_email ON wp_signups ( "user_email" ); CREATE INDEX wp_signups_user_login_email ON wp_signups ( "user_login","user_email" ); CREATE INDEX wp_signups_domain_path ON wp_signups ( "domain","path" );
