CREATE TABLE wp_users ( ID bigint(20) unsigned NOT NULL auto_increment, user_login varchar(60) NOT NULL default '', user_pass varchar(255) NOT NULL default '', user_nicename varchar(50) NOT NULL default '', user_email varchar(100) NOT NULL default '', user_url varchar(100) NOT NULL default '', user_registered datetime NOT NULL default '0000-00-00 00:00:00', user_activation_key varchar(255) NOT NULL default '', user_status int(11) NOT NULL default '0', display_name varchar(250) NOT NULL default '', PRIMARY KEY  (ID), KEY user_login_key (user_login), KEY user_nicename (user_nicename), KEY user_email (user_email)) DEFAULT CHARACTER SET latin1 COLLATE latin1_swedish_ci;
CREATE TABLE wp_users ( "ID" SERIAL NOT NULL, "user_login" VARCHAR(60) NOT NULL DEFAULT '', "user_pass" VARCHAR(255) NOT NULL DEFAULT '', "user_nicename" VARCHAR(50) NOT NULL DEFAULT '', "user_email" VARCHAR(100) NOT NULL DEFAULT '', "user_url" VARCHAR(100) NOT NULL DEFAULT '', "user_registered" TIMESTAMP NOT NULL DEFAULT '0001-01-01 00:00:00', "user_activation_key" VARCHAR(255) NOT NULL DEFAULT '', "user_status" INT NOT NULL DEFAULT '0', "display_name" VARCHAR(250) NOT NULL DEFAULT '' , PRIMARY KEY ("ID") ); CREATE INDEX wp_users_user_login_key ON wp_users ( "user_login" ); CREATE INDEX wp_users_user_nicename ON wp_users ( "user_nicename" ); CREATE INDEX wp_users_user_email ON wp_users ( "user_email" );
