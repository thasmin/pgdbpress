SELECT   wp_posts.ID FROM wp_posts  WHERE 1=1  AND wp_posts.post_type = 'post' AND ((wp_posts.post_status = 'publish'))  ORDER BY wp_posts.post_date DESC LIMIT 0, 5
SELECT wp_posts."ID" FROM "wp_posts" WHERE 1 = 1 AND wp_posts."post_type" = 'post' AND wp_posts."post_status" = 'publish' ORDER BY wp_posts."post_date" DESC LIMIT 5 OFFSET 0;
