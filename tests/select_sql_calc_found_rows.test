SELECT SQL_CALC_FOUND_ROWS  wp_posts.ID FROM wp_posts  WHERE 1=1  AND wp_posts.post_type = 'post' AND (wp_posts.post_status = 'publish' OR wp_posts.post_status = 'private')  ORDER BY wp_posts.post_date DESC LIMIT 0, 10
SELECT wp_posts."ID", COUNT(*) OVER() AS _translator_full_count FROM "wp_posts" WHERE 1 = 1 AND wp_posts."post_type" = 'post' AND  ( wp_posts."post_status" = 'publish' OR wp_posts."post_status" = 'private' ) ORDER BY wp_posts."post_date" DESC LIMIT 10 OFFSET 0;
