.setup
CREATE TEMPORARY TABLE tab1(col1 integer);
CREATE TEMPORARY TABLE tab2(col2 integer);
INSERT INTO tab1(col1) VALUES(1), (2);
INSERT INTO tab2(col2) VALUES(2), (3);

.run
DELETE t1, t2 FROM tab1 t1, tab2 t2 WHERE t1.col1 = t2.col2;

.test
SELECT * FROM t1;
[1]
SELECT * FROM t2;
[3]

.teardown
DROP TABLE tab1;
DROP TABLE tab2;

